{
  "name": "@propine/service.report",
  "version": "0.0.0",
  "private": true,
  "description": "Report service",
  "keywords": [],
  "workspaces": {
    "nohoist": []
  },
  "scripts": {
    "type-check": "tsc --noemit",
    "clean": "rimraf dist *.tsbuildinfo",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "build": "tsc -b tsconfig.json",
    "build:watch": "concurrently -n Prisma,Nodemon -c blue.dim,green.dim \"npm:prisma:generate --watch\" \"nodemon\"",
    "test": "jest --color --config jest.config.js --passWithNoTests",
    "test:cov": "jest --color --coverage --config jest.config.js --passWithNoTests",
    "predev": "npm run prisma:migrate:dev",
    "dev": "serverless offline start"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@prisma/client": "^3.12.0",
    "@propine/logger": "^0.0.0",
    "@vendia/serverless-express": "^4.5.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1113.0",
    "class-transformer": "^0.5.1",
    "fast-csv": "^4.3.6",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/lodash": "^4.14.181",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "concurrently": "^7.1.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.12.0",
    "serverless-offline": "^8.5.0",
    "ts-jest": "^27.1.4",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  }
}
