{
  "name": "@propine/cli",
  "version": "0.0.0",
  "private": true,
  "description": "CLI",
  "keywords": [],
  "workspaces": {
    "nohoist": []
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "bin": {
    "propine": "./bin/run"
  },
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "yarn build && oclif manifest"
  },
  "dependencies": {
    "@oclif/core": "^1.6.4",
    "@oclif/plugin-help": "^5.1.12",
    "aws-sdk": "^2.1111.0",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "oclif": "^2.6.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "_moduleAliases": {
    "@core": "dist/cores",
    "@common": "dist/common",
    "@commands": "dist/commands"
  },
  "oclif": {
    "bin": "propine",
    "dirname": "propine",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
