{
  "name": "@propine/cli",
  "version": "0.0.0",
  "private": true,
  "description": "CLI",
  "keywords": [],
  "workspaces": {
    "nohoist": []
  },
  "main": "dist/index.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "bin": {
    "propine": "./bin/run"
  },
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "yarn build && oclif manifest"
  },
  "dependencies": {
    "@oclif/core": "^1.6.4",
    "@oclif/plugin-help": "^5.1.12",
    "@propine/logger": "^0.0.0",
    "aws-sdk": "^2.1111.0",
    "fs-extra": "^10.0.1",
    "lodash": "^4.17.21",
    "s3-upload-stream": "^1.0.7"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/s3-upload-stream": "^1.0.2",
    "oclif": "^2.6.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "oclif": {
    "bin": "propine",
    "dirname": "propine",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
